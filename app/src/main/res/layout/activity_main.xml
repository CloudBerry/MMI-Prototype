<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context=".MainActivity">

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true" android:id="@+id/linearLayout">
        <Button
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="NYE LØP"
                android:id="@+id/button" android:layout_weight="1" android:background="#54a6ff"
                android:textColor="#ffffff"/>
        <Button android:layout_width="fill_parent" android:layout_height="wrap_content" android:text="MINE LØP"
                android:id="@+id/button2" android:layout_weight="1" android:textColor="#ffffff"
                android:background="#cccccc"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerHorizontal="true" android:layout_below="@+id/linearLayout" >
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="Osloløpet"
                      android:id="@+id/textView" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="12. Mars"
                      android:id="@+id/textView2" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="DNB Marathon"
                      android:id="@+id/textView3" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="5.April"
                      android:id="@+id/textView4" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp" android:id="@+id/ntnulop">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="NTNU 10 km"
                      android:id="@+id/textView5" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="10. April"
                      android:id="@+id/textView6" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="Kompisturen"
                      android:id="@+id/textView7" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="15. April"
                      android:id="@+id/textView8" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="Moroløpet"
                      android:id="@+id/textView9" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="20. April"
                      android:id="@+id/textView10" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="Nordmarkstraver&apos;n"
                      android:id="@+id/textView11" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="25. April"
                      android:id="@+id/textView12" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="Sognsvann Rundt"
                      android:id="@+id/textView13" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="30. April"
                      android:id="@+id/textView14" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="10 for Grethe"
                      android:id="@+id/textView15" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="5. Mai"
                      android:id="@+id/textView16" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="wrap_content"
                        android:layout_marginTop="15dp">
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="Casual Løp"
                      android:id="@+id/textView17" android:layout_alignParentTop="true"
                      android:layout_alignParentLeft="true" android:layout_alignParentStart="true"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                      android:textAppearance="?android:attr/textAppearanceLarge" android:text="8. Mai"
                      android:id="@+id/textView18" android:layout_alignParentTop="true"
                      android:layout_alignParentRight="true" android:layout_alignParentEnd="true"/>
        </RelativeLayout>
    </LinearLayout>
</RelativeLayout>
